.ui.inverted.blue.vertical.segment{:id=>'wicker_materials'}
  .ui.page.grid
    .column
      %h1{:id=>'wicker_materials'} Materials
      - @materials.each do |m|
        =m.name
        =m.description.html_safe
        -@downloads.each do |d|
          -if(d.material_id==m.id)
            %a{:href=> d.link}
              = d.file